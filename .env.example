# Copy this file to `.env` and fill in your secrets.

# OpenAI API secret token (keep private and never commit to git)
OPENAI_API_SECRET=

# Model that supports web tools/browsing via Responses API
# Common choice: gpt-4.1-mini
OPENAI_API_MODEL=gpt-4.1-mini

# Enable link-based analysis (no repo download)
# Must be true for the service to work
OPENAI_ALLOW_WEB=true
